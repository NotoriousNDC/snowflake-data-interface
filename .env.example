# Snowflake Connection Parameters
SNOWFLAKE_ACCOUNT=your_account_identifier # e.g., xy12345.us-east-1
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=your_warehouse_name
SNOWFLAKE_DATABASE=your_database_name
SNOWFLAKE_SCHEMA=your_schema_name
SNOWFLAKE_ROLE=your_role # e.g., ACCOUNTADMIN, SYSADMIN

# Optional Parameters
SNOWFLAKE_REGION=your_region # e.g., us-east-1
SNOWFLAKE_SESSION_PARAMETERS='{"QUERY_TAG": "VSCode_Extension"}'

# Security Parameters
PRIVATE_KEY_PATH=path/to/your/rsa_key.p8 # Optional: for key pair authentication
PRIVATE_KEY_PASSPHRASE=your_passphrase # Optional: if your private key is encrypted 